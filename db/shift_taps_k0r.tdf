FUNCTION altsyncram_q0c1 (address_a[9..0], address_b[9..0], clock0, clocken0, data_a[15..0], wren_a)
RETURNS ( q_b[15..0]);
FUNCTION cntr_hpf (clk_en, clock)
RETURNS ( q[9..0]);

--synthesis_resources = lut 10 M4K 4 reg 10 
SUBDESIGN shift_taps_k0r
( 
	clken	:	input;
	clock	:	input;
	shiftin[15..0]	:	input;
	shiftout[15..0]	:	output;
	taps[15..0]	:	output;
) 
VARIABLE 
	altsyncram2 : altsyncram_q0c1;
	cntr1 : cntr_hpf;

BEGIN 
	altsyncram2.address_a[] = cntr1.q[];
	altsyncram2.address_b[] = cntr1.q[];
	altsyncram2.clock0 = clock;
	altsyncram2.clocken0 = clken;
	altsyncram2.data_a[] = ( shiftin[]);
	altsyncram2.wren_a = B"1";
	cntr1.clk_en = clken;
	cntr1.clock = clock;
	shiftout[15..0] = altsyncram2.q_b[15..0];
	taps[] = altsyncram2.q_b[];
END;
